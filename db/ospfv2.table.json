{
  "table": {
    "ospfv2": {
      "type": "parent",
      "has-dependent": [
        "ospfv2-area",
        "ospfv2-network",
        "ospfv2-redistribute",
        "ospfv2-distribute-list-out"
      ],
      "keys": {
        "process-id" : {
          "type": "integer",
          "range": [1, 65535]
        }
      },
      "attributes": {
        "auto-cost": {
          "desc": "auto-cost reference bandwidth",
          "type": "integer",
          "range": [1, 4294967],
          "default": 100
        },
        "capability-opaque": {
          "type": "boolean",
          "default": false
        },
        "compatible-rfc1583": {
          "type": "boolean",
          "default": false
        },
        "default-metric": {
          "type": "integer",
          "range": [1, 16777214]
        },
        "distance": {
          "desc": "distance <1-255>",
          "type": "integer",
          "range": [1, 255],
          "default": 110
        },
        "distance-external": {
          "desc": "distance ospf external",
          "type": "integer",
          "range": [1, 255],
          "default": 110
        },
        "distance-inter-area": {
          "desc": "distance ospf inter-area",
          "type": "integer",
          "range": [1, 255],
          "default": 110
        },
        "distance-intra-area": {
          "desc": "distance ospf intra-area",
          "type": "integer",
          "range": [1, 255],
          "default": 110
        },
        "log-adjacency-changes": {
          "desc": "log-adacency-changes (detail|)",
          "type": "enum",
          "default": "",
          "null": true,
          "values": [ "", "detail", null ]
        },
        "max-metric-on-startup": {
          "desc": "max-metric router-lsa on-startup <5-86400>",
          "type": "integer",
          "range": [5, 86400]
        },
        "max-metric-on-shutdown": {
          "desc": "max-metric router-lsa on-shutdown <5-86400>",
          "type": "integer",
          "range": [5, 86400]
        },
        "abr-type": {
          "desc": "ospf abr-type",
          "type": "enum",
          "default": "cisco",
          "values": ["cisco", "ibm", "shortcut", "standard"]
        },
        "router-id": {
          "desc": "router-id A.B.C.D",
          "type": "ipv4"
        },
        "refresh-timer": {
          "desc": "refresh timer <10-1800>",
          "type": "integer",
          "range": [10, 1800],
          "default": 10
        },
        "timers-throttle-delay": {
          "desc": "timers throttle spf",
          "type": "integer",
          "range": [0, 600000],
          "default": 200
        },
        "timers-throttle-initial-holdtime": {
          "desc": "timers throttle spf",
          "type": "integer",
          "range": [0, 600000],
          "default": 1000
        },
        "timers-throttle-maximum-holdtime": {
          "desc": "timers throttle spf",
          "type": "integer",
          "range": [0, 600000],
          "default": 10000
        }
      }
    }
  }
}
