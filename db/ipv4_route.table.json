{
  "table": {
    "ipv4-route": {
      "type": "master",
      "has-association": {
        "ipv4-route-nexthop": null,
        "nexthop-ipv4": {
          "through": "ipv4-route-nexthop"
        }
      },
      "order": "address, masklen",
      "keys": {
        "address": {
          "type": "ipv4"
        },
        "masklen": {
          "type": "integer",
          "range": [0, 32]
        }
      }
    }
  }
}
