{
  "table": {
    "ospfv2-area": {
      "type": "dependent",
      "belongs-to": {
        "ospfv2": {
          "type": "table"
        }
      },
      "has-many": {
        "ospfv2-area-vlink": {
          "type": "table"
        }
      },
      "alias": "area",
      "keys": {
        "area-id": {
          "type": "integer",
          "range": [0, 4294967295]
        }
      },
      "attributes": {
        "authentication": {
          "desc": "area authentication",
          "type": "enum",
          "values": ["message-digest", ""]
        },
        "default-cost": {
          "desc": "area default-cost",
          "type": "integer",
          "range": [0, 16777215],
          "default": 1
        },
        "export-list": {
          "type": "string"
        },
        "filter-list-prefix-in": {
          "type": "string"
        },
        "filter-list-prefix-out": {
          "type": "string"
        },
        "import-list": {
          "type": "string"
        },
        "shortcut": {
          "type": "enum",
          "values": ["default", "disable", "enable"],
          "default": "default"
        },
        "stub": {
          "type": "boolean",
          "default": false
        },
        "stub-no-summary": {
          "type": "boolean",
          "default": false
        }
      }
    }
  }
}
