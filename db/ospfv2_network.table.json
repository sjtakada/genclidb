{
  "table": {
    "ospfv2-network": {
      "type": "dependent",
      "alias": "network",
      "belongs-to": [
        "ospfv2"
      ],
      "order": "address, masklen DESC",
      "keys": {
        "address": {
          "type": "ipv4"
        },
        "masklen": {
          "type": "integer",
          "range": [0, 32]
        }
      },
      "attributes": {
        "area-id": {
          "type": "integer",
          "range": [0, 4294967295],
          "null": false
        }
      }
    }
  }
}
