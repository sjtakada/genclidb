{
  "table": {
    "ipv4-route-nexthop": {
      "type": "association",
      "belongs-to": [
        "ipv4-route",
        "nexthop-ip"
      ],
      "belongs-to-polymorphic": [
        "ipv4-reachable"
      ],
      "attributes": {
        "distance": {
          "type": "integer",
          "range": [1, 255]
        }
      }
    }
  }
}
