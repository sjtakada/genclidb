{
  "table": {
    "ipv4-route-nexthop": {
      "type": "association",
      "belongs-to": {
        "table": "ipv4-route",
        "polymorphic": {
          "interface": "ipv4-reachable",
          "table": [
            "nexthop-ipv4",
            "nexthop-if"
          ]
        }
      },
      "attributes": {
        "distance": {
          "type": "integer",
          "range": [1, 255]
        }
      }
    }
  }
}
