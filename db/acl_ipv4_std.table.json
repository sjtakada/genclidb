{
  "table": {
    "acl-ipv4-std": {
      "type": "parent",
      "has-many": {
        "acl-ipv4-std-rule": {
          "type": "table"
        }
      },
      "keys": {
        "number": {
          "type": "integer",
          "range": [1, 1999]
        }
      }
    },
    "acl-ipv4-std-rule": {
      "type": "dependent",
      "alias": "rule",
      "belongs-to": {
        "acl-ipv4-std": {
          "type": "table"
        }
      },
      "keys": {
        "rule-type": {
          "type": "enum",
          "values": ["rule", "remark"]
        },
        "address": {
          "type": "ipv4"
        },
        "wildcard-bits": {
          "type": "ipv4"
        }
      },
      "attributes": {
        "permit-deny": {
          "type": "enum",
          "values": ["permit", "deny"]
        },
        "remark": {
          "type": "string"
        }
      }
    }
  }
}
