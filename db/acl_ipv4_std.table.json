{
  "table": {
    "acl-ipv4-std": {
      "type": "parent",
      "has-many": {
        "acl-ipv4-std-entry": {
          "type": "table"
        }
      },
      "order": "number",
      "keys": {
        "number": {
          "type": "integer",
          "range": [1, 1999]
        }
      }
    },
    "acl-ipv4-std-entry": {
      "type": "dependent",
      "alias": "entry",
      "belongs-to": {
        "acl-ipv4-std": {
          "type": "table",
          "destroy-upon": true
        }
      },
      "has-one": {
        "acl-ipv4-std-rule": {
          "type": "table"
        }
      },
      "order": "seq",
      "keys": {
        "seq": {
          "type": "integer",
          "auto": {
            "step": 10
          }
        }
      }
    },
    "acl-ipv4-std-rule": {
      "type": "dependent",
      "alias": "rule",
      "belongs-to": {
        "acl-ipv4-std-entry": {
          "type": "table",
          "destroy-upon": true
        }
      },
      "keys": {
        "address": {
          "type": "ipv4"
        },
        "wildcard-bits": {
          "type": "ipv4"
        }
      },
      "attributes": {
        "permit-deny": {
          "type": "enum",
          "values": ["permit", "deny"]
        },
        "remark": {
          "type": "string"
        }
      }
    }
  }
}
