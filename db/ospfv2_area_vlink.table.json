{
  "table": {
    "ospfv2-area-vlink": {
      "type": "dependent",
      "parent": "ospfv2-area",
      "has-dependent": [
        "ospfv2-area-vlink-md5-key"
      ],
      "alias": "vlink",
      "keys": {
        "nbr-id": {
          "type": "ipv4"
        }
      },
      "attributes": {
        "authentication-key": {
          "type": "string"
        },
        "authentication": {
          "type": "enum",
          "default": "",
          "values": ["", "null", "message-digest"]
        },
        "hello-interval": {
          "type": "integer",
          "range": [1, 65535]
        },
        "dead-interval": {
          "type": "integer",
          "range": [1, 65535]
        },
        "retransmit-interval": {
          "type": "integer",
          "range": [1, 65535]
        },
        "transmit-delay": {
          "type": "integer",
          "range": [1, 65535]
        }
      }
    }
  }
}
