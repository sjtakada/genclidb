  "interface-cmd": {
    "token": {
      "interface": {
        "id": "1",
        "type": "keyword",
        "help": "Select an interface to configure"
      },
      "WORD:2": {
        "id": "2",
        "type": "string",
        "help": "Interface's name",
        "dynamic": {
          "http": {
            "method": "GET",
            "path": "show/interfaces",
            "format": "json"
          },
          "cache": true
        }
      }
    },
    "command": [
      {
        "defun": "interface WORD:2",
        "mode": [
          "CONFIG-NODE"
        ],
        "bind": [
          "IFNAME = WORD:2"
        ],
        "action": [
          {
            "mode": {
              "name": "INTERFACE-NODE",
              "params": [
                "IFNAME"
              ]
            },
            "http": {
              "method": "PUT",
              "path": "interfaces/:IFNAME"
            }
          }
        ]
      }
    ]
  },
